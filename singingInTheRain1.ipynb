    "plt.scatter(ratios.index,ratios,marker='o')\n",
    "plt.xlabel(\"Date\",size=18)\n",
    "plt.ylabel('ratio of happy songs to all songs played',size=16)\n",
    "plt.grid(True)\n",
    "plt.show()"
